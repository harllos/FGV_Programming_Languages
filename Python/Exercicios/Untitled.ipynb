   "source": [
    "qsort <- function(v){\n",
    "  if ( length(v) > 1 ) \n",
    "  {\n",
    "    pivot <- (min(v) + max(v))/2.0                            # Could also use pivot <- median(v)\n",
    "    c(qsort(v[v < pivot]), v[v == pivot], qsort(v[v > pivot])) \n",
    "  } else v\n",
    "}\n",
    " \n",
    "N <- 100\n",
    "vs <- runif(N)\n",
    "system.time(u <- qsort(vs))\n",
    "print(u)"
   ]
